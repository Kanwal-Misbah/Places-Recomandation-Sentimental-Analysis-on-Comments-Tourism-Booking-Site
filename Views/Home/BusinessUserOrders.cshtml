@model IEnumerable<Fin.Models.Booking>

@{
    ViewBag.Title = "BusinessUserOrders";
}
@{
    Layout = "~/Views/Shared/_LayoutBusinessUser.cshtml";

}

<h2>BusinessUserOrders</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PACKAGE.Package_title)
        </th>
        <th>
            Business Name
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Booking_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.No_of_Members)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Departure)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Total_Payment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Payment_status)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PACKAGE.Package_title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PACKAGE.Business_User.Bus_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Booking_date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.No_of_Members)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Departure)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Total_Payment)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Payment_status)
            </td>
            
        </tr>
    }

</table>
